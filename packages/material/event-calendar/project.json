{
  "name": "material/event-calendar",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/material/event-calendar/src",
  "prefix": "mat",
  "projectType": "library",
  "tags": ["scope:ui"],
  "targets": {
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../../coverage/packages/material/event-calendar"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "packages/material/event-calendar/.storybook",
        "browserTarget": "material/event-calendar:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/material/event-calendar",
        "configDir": "packages/material/event-calendar/.storybook",
        "browserTarget": "material/event-calendar:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "dependsOn": ["build-storybook"],
      "options": {
        "buildTarget": "material/event-calendar:build-storybook",
        "staticFilePath": "dist/storybook/material/event-calendar",
        "spa": true
      },
      "configurations": {
        "ci": {
          "buildTarget": "material/event-calendar:build-storybook:ci"
        }
      }
    }
  }
}
